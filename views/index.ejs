<body>
    <h1>Login</h1>
    <form action="/login" method="post">
      <div>
        <label for="email">Email:</label>
        <input type="email" id="email" name="email">
      </div>
      <div>
        <label for="password">Senha:</label>
        <input type="password" id="password" name="password">
      </div>
      <div>
        <button type="submit">Entrar</button>
      </div>
    </form>
  </body>
</html>